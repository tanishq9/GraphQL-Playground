# Union

type Query {
    search: [Result]
}

union Result = Fruit | Electronics | Book

type Fruit {
    description: String
    expiryDate: String
}

type Electronics {
    id: ID!
    description: String
    price: Int
    brand: Brand
}

type Book {
    title: String
    author: String
}

enum Brand {
    APPLE,
    SAMSUNG
}

